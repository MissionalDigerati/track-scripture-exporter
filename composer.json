{
    "name": "missional-digerati/track-scripture-exporter",
    "description": "This tool provides an easy way to export Bible verses for your Christian track development.",
    "type": "project",
    "license": "GPL-3",
    "authors": [
        {
            "name": "Codemis",
            "email": "johnathan@missionaldigerati.org"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "faithcomesbyhearing/dbt-sdk-php",
                "version": "dev-master",
                "source": {
                    "url": "git@bitbucket.org:faithcomesbyhearing/dbt-sdk-php.git",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        }
    ],
    "require": {
      "php": ">=7.0",
      "faithcomesbyhearing/dbt-sdk-php": "dev-master"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.2"
    },
    "scripts": {
      "lint": "vendor/bin/phpcs --colors --standard=psr2 src/"
    }
}
